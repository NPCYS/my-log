<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.mqz.mapper.TagMapper">
    <update id="tombstone">
        UPDATE sg_tag
        SET del_flag = 1
        WHERE id =#{tagId}
    </update>
</mapper>